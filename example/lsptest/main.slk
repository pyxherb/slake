module hostext;

class AnotherGeneric<T, U> {
	class ChildGeneric<V, T> {
		pub T x;

		pub void setX(T x) {
			this.x = x;
			return;
		}

		T getX() {
			return this.x;
		}
	}
}

pub i32 main() {
	AnotherGeneric<i32, f32>.ChildGeneric<f32, i32> x = new AnotherGeneric<i32, f32>.ChildGeneric<f32, i32>();
	x.setX(123);
	x.getX();

	while(x.x--) {
		yield x.x;
	}

	return 114514;
}
